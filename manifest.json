{
    "name": "Plateau",
    "id": "plateau",
    "packaging_format": 1,
    "description": {
        "en": "Plateau, a collaborative workshop documentation and organization platform.",
        "fr": "Plateau, un outil de documentation et d'organisation d'ateliers collaboratifs."
    },
    "version": "1.0~ynh1",
    "url": "https://plateau.latelier-des-chercheurs.fr/",
    "upstream": {
        "license": "free",
        "website": "https://github.com/l-atelier-des-chercheurs/PLATEAU",
        "demo": "https://github.com/l-atelier-des-chercheurs/PLATEAU",
        "admindoc": "https://github.com/l-atelier-des-chercheurs/PLATEAU",
        "userdoc": "https://github.com/YunoHost-Apps/plateau_ynh",
        "code": "https://github.com/l-atelier-des-chercheurs/PLATEAU"
    },
    "license": "free",
    "maintainer": {
        "name": "Guillaume Bouzige",
        "email": "guillaume.bouzige+yunohost@ddataa.org"
    },
    "requirements": {
        "yunohost": ">= 11.0.0"
    },
    "multi_instance": false,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/plateau",
                "default": "/plateau"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
